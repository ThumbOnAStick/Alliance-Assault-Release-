<?xml version="1.0" encoding="UTF-8"?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>[SR]Factional War</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/WorldObjectDef[defName="SrSiteFactionWarShelling"]/comps</xpath>
					<value>
							<li Class="Non_Plyaer_Assault.WorldObjectCompProperties_FactionWarObserver">
							</li>
					</value>
				
				</li>
		
				<li Class="PatchOperationAdd">
					<xpath>/Defs/WorldObjectDef[defName="SrSiteFactionWarTempCamp"]/comps</xpath>
					<value>
						<li Class="Non_Plyaer_Assault.WorldObjectCompProperties_FactionWarObserver">
						</li>
					</value>
					
				</li>
				
				<li Class="PatchOperationAdd">
					<xpath>/Defs/WorldObjectDef[defName="SrSiteFactionWarContention"]/comps</xpath>
					<value>
						<li Class="Non_Plyaer_Assault.WorldObjectCompProperties_FactionWarObserver">
						</li>
					</value>
				</li>
			</operations>
			
			
		</match>
	</Operation>
</Patch>